{
    "name": "labolagen/laravel-generator",
    "type": "library",
    "description": "Labolagen, laravel-boilerplate + laravel-generator",
    "keywords": [
        "laravel",
        "api",
        "model",
        "request",
        "migration",
        "model",
        "crud",
        "repository",
        "view",
        "test",
        "generator",
        "swagger",
        "labolagen"
    ],
    "license": "MIT",
    "authors": [
        {
            "name": "Mitul Golakiya",
            "email": "me@mitul.me"
        },
        {
            "name": "Skys215",
            "email": "skys215@gmail.com"
        }
    ],
    "require": {
        "php": ">=7.2",
        "illuminate/support": "^6.0.0",
        "labolagen/coreui-templates": "dev-develop"
    },
    "require-dev": {
        "phpunit/phpunit": "~5.0",
        "mockery/mockery": "~0.9"
    },
    "autoload": {
        "psr-4": {
            "Labolagen\\Generator\\": "src/"
        },
        "files": [
            "src/helpers.php"
        ]
    },
    "autoload-dev": {
        "psr-4": {
            "Tests\\": "tests/"
        }
    },
    "extra": {
        "laravel": {
            "providers": [
                "\\Labolagen\\Generator\\LabolagenGeneratorServiceProvider"
            ]
        }
    }
}
